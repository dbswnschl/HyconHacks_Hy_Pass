<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hypass.hycon.hypass">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppFullScreenTheme">
        <meta-data
            android:name="android.max_aspect"
            android:resizeableActivity="true"
            android:value="2.1" />

        <activity android:name=".Activities.InitActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SignUpActivity" />
        <activity android:name=".Activities.SignInActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.WarnningActivity" />
        <activity android:name=".Activities.MnemonicActivity" />
        <activity android:name=".Activities.FriendListActivity"></activity>
    </application>

</manifest>